{
  "name": "collaborative-workspace",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/frontend",
    "apps/backend"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend",
    "dev:backend": "npm --workspace @workspace/backend run dev",
    "dev:frontend": "npm --workspace @workspace/frontend run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "npm --workspace @workspace/backend run build",
    "build:frontend": "npm --workspace @workspace/frontend run build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
